	if(error == "success"){
		displayError(newError, "highlight", "DI has been Updated!!!");
		$("#dichange_dialog").dialog("close");
		updatePageBehaviour();		
	}
	else{
		displayError(newError, "error", "<strong>Error !</strong> - Unable to update the DI due to " + decodeURI(xmlData.find("errtxt").text()));
	}
}

